 @inbook{Morel-ICM, address={Zuerich, Switzerland}, title={A1-algebraic topology}, ISBN={978-3-03719-022-7}, url={http://www.ems-ph.org/doi/10.4171/022-2/49}, DOI={10.4171/022-2/49}, abstractNote={We present some recent results in A1-algebraic topology, which means both in A1-homotopy theory of schemes and its relationship with algebraic geometry. This refers to the classical relationship between homotopy theory and (differential) topology. We explain several examples of “motivic” versions of classical results: the theory of the Brouwer degree, the classiﬁcation of A1-coverings through the A1-fundamental group, the Hurewicz Theorem and the A1-homotopy of algebraic spheres, and the A1-homotopy classiﬁcation of vector bundles. We also give some applications and perspectives.}, booktitle={Proceedings of the International Congress of Mathematicians Madrid, August 22–30, 2006}, publisher={European Mathematical Society Publishing House}, author={Morel, Fabien}, editor={Sanz-Solé, Marta and Soria, Javier and Varona, Juan Luis and Verdera, Joan}, year={2007}, month=may, pages={1035–1059}, language={en} }


